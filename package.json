{
  "name": "duke-togo",
  "version": "1.0.0",
  "description": "Duke Togo",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "lint": "npm-run-all --parallel lint:*",
    "audit": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "lint:tslint": "./node_modules/tslint/bin/tslint -p .",
    "watch": "tsc -w",
    "start-dev": "NODE_ENV=development ts-node-dev ./src/bin/main.ts",
    "start-dev-debug": "NODE_ENV=development ts-node-dev --inspect=0.0.0.0:5860 -- ./src/bin/main.ts",
    "test": "./node_modules/mocha/bin/mocha -r ts-node/register src/test/**/*.spec.ts src/test/**/**/*.spec.ts src/test/**/**/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbangert/duketogo.git"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "Ben Bangert",
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/bbangert/duketogo/issues"
  },
  "homepage": "https://github.com/bbangert/duketogo#readme",
  "readmeFilename": "README.md",
  "dependencies": {
    "@sentry/integrations": "^5.15.4",
    "@sentry/node": "^5.15.4",
    "@types/debug": "^4.1.5",
    "bufferutil": "^4.0.1",
    "convict": "^5.2.0",
    "debug": "^4.1.1",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "ramda": "^0.27.0",
    "sqlite3": "^4.1.1",
    "tslib": "^1.11.1",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chance": "^1.0.10",
    "@types/convict": "^4.2.1",
    "@types/graphql": "^14.5.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.0",
    "@types/ramda": "^0.27.3",
    "@types/sinon": "^9.0.0",
    "@types/ws": "^7.2.4",
    "@types/yargs": "^15.0.4",
    "audit-filter": "^0.5.0",
    "chai": "^4.2.0",
    "chance": "^1.1.4",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "ts-sinon": "^1.2.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3"
  }
}
